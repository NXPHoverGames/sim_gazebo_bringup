{	"setup":{
		"autopilot": {
			"autopilot_build_params_0": {
				"repo": "git@github.com:NXPHoverGames/PX4-Autopilot.git",
				"name": "NXP-Autopilot",
				"version": "pr-cupcar-v2",
				"clone": 1,
				"source_gazebo_plugins": 0,
				"build_type": "px4_sitl_rtps",
				"build_prefix": "DONT_RUN=1",
				"build_postfix": ""
			}
		},
		"gazebo": {
			"gazebo_models": {
				"gazebo_model_repo_0":{
					"repo": "git@github.com:NXPHoverGames/nxp_gazebo.git",
					"version": "fire_turtle",
					"name": "nxp_gazebo"
				},
				"gazebo_model_repo_1":{
					"repo": "git@github.com:osrf/gazebo_models.git",
					"version": "master",
					"name": "osrf/models"
				}
			},
			"gazebo_plugins": {
				"gazebo_plugin_repo_0":{
					"repo": "git@github.com:rudislabs/PX4-SITL_gazebo.git",
					"version": "master",
					"name": "nxp_gazebo_plugins",
					"workspace_relative_mavlink": "NXP-Autopilot/mavlink/include/",
					"source_gazebo_plugins": 0
				}
			}
		},
		"ros2": {
			"ros2_repo_0":{
				"repo": "git@github.com:NXPHoverGames/px4_msgs.git",
				"version": "pr-cupcar",
				"build_package": "px4_msgs",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_1":{
				"repo": "git@github.com:NXPHoverGames/px4_ros_com.git",
				"version": "pr-cupcar",
				"build_package": "px4_ros_com",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_2":{
				"repo": "git@github.com:NXPHoverGames/nxp_cup_vision.git",
				"version": "fire_turtle",
				"build_package": "nxp_cup_vision",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			}
		},
		"system": {
			"connections": {
				"connection_0":{
					"force_unbind": true,
					"transport": "udp",
					"ports": [2019, 2020, 2021, 2022, 2023, 2024, 2025, 2026]
				},
				"connection_1":{
					"force_unbind": false,
					"transport": "tcp",
					"ports": []
				}
			},
			"set_environment": {
				"environment_0": {
					"variable": "GAZEBO_MODEL_DATABASE_URI",
					"value": "",
					"method": "overwrite"
				},
				"environment_1": {
					"variable": "GAZEBO_RESOURCE_PATH",
					"value": "/usr/share/gazebo-11",
					"method": "prepend"
				},
				"environment_2": {
					"variable": "JAVA_HOME",
					"value": "/usr/lib/jvm/java-8-openjdk-amd64/",
					"method": "ignore"
				},
				"environment_3": {
					"variable": "GAZEBO_PLUGIN_PATH",
					"value": "/opt/ros/foxy/lib",
					"method": "prepend"
				},
				"environment_4": {
					"variable": "PX4_SIM_SPEED_FACTOR",
					"value": "0.7",
					"method": "prepend"
				}
			}
		}
	},
	"verbose": {
		"gzclient": true,
		"gzserver": true
	},
	"nodes": {
		"ros2_node_0":{
			"timing": "post-spawn",
			"package": "nxp_cup_vision",
			"executable": "nxp_track_vision",
			"name": "nxp_track_vision_0",
			"output": "screen",
			"parameters": [	{"pyramid_down": 0},
						{"camera_image": "trackImage0"},
						{"debug_image": "debugImage0"},
						{"namespace": "cupcar0"}
						]
		},
		"ros2_node_2":{
			"timing": "post-spawn",
			"package": "rqt_image_view",
			"executable": "rqt_image_view",
			"name": "rqt_image_view_0",
			"output": "screen",
			"parameters": []
		}
	},
	"world_params": {
		"gazebo_name": "nxp_gazebo",
		"generate_world": 1,
		"generate_params":{
			"world_name": "nxp_raceway_octagon",
			"sdf_version": "1.6",
			"use_spherical_coords": 1,
			"lat_lon_alt": [39.8039, -84.0606, 244],
			"sun_model": "sunUTC",
			"sun_utc_date": "1904/09/20/17:30",
			"ambient_light": 0.5,
			"background_light": 0.15,
			"shadows": 1,
			"cloud_speed": 12,
			"wind_speed": "NotSet",
			"fog_params": {
				"type": "linear",
				"start_m": 1.5,
				"end_m": 50.5,
				"density": 2.5
			},			
			"embedded_models": {
			},
			"video_widget": "NotSet",
			"set_physics": 0,
			"update_rate": 100,
			"realtime_factor": 1.0,
			"ode_threads": 2
		}
	},
	"models": {
		"model_params_0": {
			"gazebo_name": "nxp_gazebo",
			"autopilot_name": "NXP-Autopilot",
			"rtps_agent_args": "NotSet",
			"autopilot_build_type": "px4_sitl_rtps",
			"spawn_pose": [0, 0, 0.2, 0, 0, 0],
			"instance": 0,
			"generate_params":{
				"controller": "px4",
				"base_model": "nxp_cupcar",
				"sdf_version": 1.6,
				"mavlink_tcp_port": 4560,
				"mavlink_udp_port": 14560,
				"qgc_udp_port": 14550,
				"sdk_udp_port": 14540,
				"serial_enabled": 0,
				"serial_device": "/dev/ttyACM0",
				"serial_baudrate": 921600,
				"enable_lockstep": 1,
				"camera_image": "trackImage0",
				"namespace": "cupcar0",
				"hil_mode": 0,
				"model_name": "NotSet"
			}
		}
	}
}
